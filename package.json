    "main": "index.js",
    "scripts": {
        "install:pkg": "npm install",
        "start": "ts-node ./src/index.ts",
        "dev": "ts-node-dev -r tsconfig-paths/register ./src/index.ts",
        "build": "tsc && tsc-alias",
        "test": "jest",
        "test:watch": "jest --watch --onlyChanged",
        "lint": "eslint . --ext .ts",
        "fix": "eslint . --ext .ts --fix",
        "check-types": "tsc -p tsconfig.json --noEmit",
