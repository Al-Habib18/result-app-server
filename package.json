        "migrate:prod": "prisma migrate deploy",
        "prepare": "husky"
    },
    "dependencies": {
        "body-parser": "^1.20.2",
        "config": "^3.3.11",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
