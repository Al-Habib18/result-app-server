        "module-alias": "^2.2.3",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "pdfjs-dist": "^3.10.111",
        "swagger-ui-express": "^5.0.0",
        "yamljs": "^0.3.0",
        "zod": "^3.23.4"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.2.1",
        "@commitlint/config-conventional": "^19.1.0",
        "@prisma/client": "^5.10.1",
        "@types/config": "^3.3.4",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/jest": "^29.5.12",
        "@types/morgan": "^1.9.9",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.11.19",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/yamljs": "^0.2.34",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "lint-staged": "^15.2.2",
        "prisma": "^5.12.1",
        "supertest": "^6.3.4",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "ts-node-dev": "^2.0.0",
        "tsc": "^2.0.4",
        "tsc-alias": "^1.8.8",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.3.3"
